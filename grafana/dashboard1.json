        {
          "query": "from(bucket: \"batteries\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"battery\")\n  |> filter(fn: (r) => r[\"_field\"] == \"V\" or r[\"_field\"] == \"Ain\" or r[\"_field\"] == \"Aout\")\n  |> filter(fn: (r) => r[\"host\"] == \"rarity\")\n  |> filter(fn: (r) => r[\"name\"] == \"battery\")\n  |> filter(fn: (r) => r[\"topic\"] == \"battery/JKBMS1\")\n  |> aggregateWindow(every: v.windowPeriod, fn: mean, createEmpty: false)\n  |> pivot(\n      rowKey: [\"_time\"],\n      columnKey: [\"_field\"],\n      valueColumn: \"_value\"\n  )\n  |> map(fn: (r) => ({\n      r with\n      _field: \"power\",\n      _value: r.V * (r.Ain + r.Aout)\n  }))\n  |> integral(unit: 1h)\n  |> yield(name: \"energy\")",
          "refId": "A"
        }
      ],
      "title": "Total Energy (W*h) for time period",
      "type": "gauge"
    },
